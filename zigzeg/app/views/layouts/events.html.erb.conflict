<!DOCTYPE HTML>
<html lang="en" >
<head>
<meta charset="utf-8" />
<title>Zigzeg - <%= @listing.name.titleize rescue '' %></title>
<meta name="robots" content="index, follow">
<meta name="keywords" content="">
<meta name="description" content="">
<meta http-equiv="X-UA-Compatible" content="chrome=1">

<!-- favicon -->
<link rel="shortcut icon" href="/favicon.ico" />

<%= stylesheet_link_tag "style" %>
<%= stylesheet_link_tag "/javascripts/fancybox/jquery.fancybox-1.3.4.css", :media => "screen" %>
<%= stylesheet_link_tag "/javascripts/themes/base/jquery.ui.tabs.css" %>

<%= javascript_include_tag "modernizr.custom" %>
<%= javascript_include_tag "jquery-1.6.2" %>
<%= javascript_include_tag "jquery.autocomplete.pack" %>
<%= javascript_include_tag "ui/jquery.ui.core.js" %>
<%= javascript_include_tag "ui/jquery.ui.widget.js" %>
<%= javascript_include_tag "ui/jquery.ui.mouse.js" %>
<%= javascript_include_tag "ui/jquery.ui.tabs.js" %>
<%= javascript_include_tag "jquery.validate" %>
<%= javascript_include_tag "fancybox/jquery.fancybox-1.3.4.js" %>
<%= javascript_include_tag "jquery.rating" %>
<%= javascript_include_tag "jquery.preloader" %>
<%= javascript_include_tag "common" %>
<%= javascript_include_tag "endPage" %>
<%= javascript_include_tag "jquery.countdown" if controller_name == "deals" %>
<%= javascript_include_tag "dealsEnd" if controller_name == "deals" %>

<%= javascript_include_tag 'rails' %>
<%= csrf_meta_tag %>
<!--[if IE]>
  <script type="text/javascript"> Cufon.now(); </script>  
  <script src="/js/ie-fix.js" type="text/javascript"></script>
<![endif]-->

<script type="text/javascript"
  src="http://maps.googleapis.com/maps/api/js?key=AIzaSyDJT0nwhhhJuvHJC8yN0DqLcc5GNAlXRYs&sensor=true">
</script>

</head>

<body>
<div class="bgGray">
  <div class="fcontainer_12 pad_bot20 pad_top" id="topArea">
    <%= render "/globals/top" %>
</div>

<%= render "/globals/searchform" %>

<div class="dealsTopBg" id="topLine">
  <div class="fcontainer_13 clearfix ">
    <!--start intro text-->
    <section id="intro" class="fgrid_12">
      <div class="fleft alpha pad_bot width70">
        <div class="full_width margin_left_20">
        <!-- text auto fill area-->
          <div class="jtextfillDeals white">
            <% if @listing.listing_type == "event" %>
	      <span><%= @event.name.titleize %></span><br>
            <% else %>
              <span><%= @deal.name.titleize %></span><br>
            <% end %>
            <div class="fleft"> 
              <div id="fb-root" class=""></div>
		<script>(function(d, s, id) {
                    var js, fjs = d.getElementsByTagName(s)[0];
                    if (d.getElementById(id)) {return;}
                    js = d.createElement(s); js.id = id;
                    js.src = "//connect.facebook.net/en_US/all.js#appId=235306483191252&xfbml=1";
                    fjs.parentNode.insertBefore(js, fjs);
                  }(document, 'script', 'facebook-jssdk'));</script>
              <div class="fb-like fleft" data-send="false" data-width="260" data-show-faces="false" data-colorscheme="dark" data-font="arial"></div>
            </div>

              <div class="fleft">
                <span class="bgDivider margin_left_10"> </span>
                <a href="#loginMessage" id="zigMessagePop" name="modal" class="absolute"></a>
                <a id="zigMe" class="zigThis cursor" data-id="<%=@listing.id%>">zig this</a>
              </div>
	      

          </div>
        </div>
      </div>

      <% unless @event.nil? %>
        <%= render :partial =>  "/globals/countdown_event", :locals => { :event => @event } %>
      <% else %>
        <%= render :partial =>  "/globals/countdown_deal", :locals => { :deal => @deal } %>
      <% end %>

    </section>
    <!--end intro text-->
  </div>
</div>
<!-- end bg gray  -->


<!--start page_wrap-->
<div class="fcontainer_12 clearfix  relative">
  <% event_deal = @event ||= @deal %>
  <%= render :partial =>  "/globals/event_breadcrumbs", :locals => { :object => event_deal } %>

    <!--start content-->
    <section id="content" class="fgrid_12 clearfix relative">
      <div id="main" class="width70 alpha">
        <%= yield %>
      </div>
      <!--end main-->
      <%= render :partial =>  "/globals/sidebar", :locals => { :listing => @listing } %>
    </section>
    <!--end content-->
    <div class="clear"></div>
</div>
<!--end page_wrap-->

<%= render "/globals/footer" %>
<%= render :partial =>  "/globals/contact_form", :locals => { :place => @place } %>

<div id="boxes">
  <%= render "/globals/modal_login" %>
  <%= render "/globals/modal_forget" %>
  <%= render "/globals/modal_signup" %>
  <%= render "/globals/modal_report" %>
  <div id="mask"></div>
</div>

</body>
</html>
