#login_form
  %center
    %h3 Etroduce Login
    -form_tag sessions_path :method => :post do
      %table
        %tr
          %td Email:
          %td
            = text_field_tag "email", "", :style => "width: 190px"
        %tr
          %td Password:
          %td
            = password_field_tag "password","", :style => "width: 190px"
        %tr
          %td
          %td
            = submit_tag "Login"
