<script type="text/javascript">

  var _gaq = _gaq || [];
  _gaq.push(['_setAccount', 'UA-24618363-1']);
  _gaq.push(['_trackPageview']);

  (function() {
    var ga = document.createElement('script'); ga.type = 'text/javascript'; ga.async = true;
    ga.src = ('https:' == document.location.protocol ? 'https://ssl' : 'http://www') + '.google-analytics.com/ga.js';
    var s = document.getElementsByTagName('script')[0]; s.parentNode.insertBefore(ga, s);
  })();

</script>

</head>
<body>

<div id="notice_or_error">
  <%= render :partial => "globals/notice_or_error" %>
</div>

<div id="page_wrap">
  <div class="packer">

    <div id="header">
      <%= render :partial => "globals/header"%>
    </div>
    
    <div id="home_content">
      <%= yield -%>
    </div>
    
  </div>
</div>

<div id="footer">
  <%= render :partial => "globals/footer" %>
</div>

</body>
</html>